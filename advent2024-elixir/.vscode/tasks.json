{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "mix",
			"task": "Run tests in current file",
			"problemMatcher": [
				"$mixCompileError",
				"$mixCompileWarning",
				"$mixTestFailure"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "mix: Run tests in current file"
		}
	]
}